    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.46.0",
    "istanbul": "^0.4.5",
    "jscoverage": "^0.6.0",
    "lodash.range": "^3.2.0",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0"
  },
  "scripts": {
    "test": "npm run compile && cross-env NODE_ENV=test mocha",
    "compile": "rimraf lib && babel --out-dir lib --source-maps true src",
    "prepublish": "npm run precommit && npm run compile",
    "precommit": "npm test && npm run lint",
    "cover": "npm run compile && istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "coverage": "npm run compile && LIQUID_NODE_COVERAGE=1 mocha --compilers js:babel-core/register -r jscoverage --reporter mocha-lcov-reporter test | coveralls",
    "coverage-report": "npm run compile && LIQUID_NODE_COVERAGE=1 mocha --compilers js:babel-core/register -r jscoverage --covout html test",
    "lint": "eslint src test",
    "lint:fix": "eslint --fix src test",
    "lint:coffee": "coffeelint src/**/*.coffee"
  }
}
